{
  "name": "sudoku-client",
  "version": "1.0.0",
  "description": "client de base pour le projet 2/Sudoku",
  "scripts": {
    "start": "npm run-script copy-assets && tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "lint": "tslint app/**/*.ts app/*.ts --force -t verbose",
    "test": "npm run-script copy-assets && tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "npm run-script copy-assets && tsc && karma start karma.conf.js --single-run || npm run-script coverage",
    "coverage": "remap-istanbul -i coverage/coverage-final.json -o coverage -t html",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "clean": "rimraf out node_modules",
    "copy-assets": "gulp assets",
    "postinstall": "rimraf false"
  },
  "dependencies": {
    "@angular/common": "2.4.10",
    "@angular/compiler": "2.4.10",
    "@angular/core": "2.4.10",
    "@angular/forms": "2.4.10",
    "@angular/http": "2.4.10",
    "@angular/material": "2.0.0-beta.2",
    "@angular/platform-browser": "2.4.10",
    "@angular/platform-browser-dynamic": "2.4.10",
    "@angular/router": "3.4.10",
    "angular-in-memory-web-api": "0.2.4",
    "bootstrap": "3.3.7",
    "core-js": "2.4.1",
    "hammerjs": "2.0.8",
    "jquery": "3.1.1",
    "module-id": "2.0.4",
    "rxjs": "5.0.1",
    "socket.io": "1.7.2",
    "systemjs": "0.19.40",
    "zone.js": "0.7.4"
  },
  "devDependencies": {
    "@types/chai": "3.4.34",
    "@types/mocha": "2.2.36",
    "@types/node": "6.0.46",
    "lodash": "4.16.4",
    "canonical-path": "0.0.2",
    "concurrently": "3.1.0",
    "http-server": "0.9.0",
    "gulp": "3.9.1",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-firefox-launcher": "1.0.0",
    "lite-server": "2.2.2",
    "browser-sync": "2.18.8",
    "rimraf": "2.5.4",
    "tslint": "4.2.0",
    "tslint-eslint-rules": "3.2.3",
    "typescript": "2.0.10",
    "chai": "3.5.0",
    "codelyzer": "2.0.0-beta.4",
    "istanbul": "0.4.5",
    "karma-chai": "0.1.0",
    "karma-coverage": "1.1.1",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.1",
    "mocha": "3.2.0",
    "remap-istanbul": "0.8.4"
  }
}